{
  "swagger": "2.0",
  "info": {
    "title": "Generic Telemetry reporter API",
    "description": "Generic API for telemetryd service.",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "ReporterAPI"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/telemetry/GenericReport": {
      "post": {
        "summary": "GenericReport",
        "description": "GenericReport is used to submit generic telemetry reports to telemetryd service.",
        "operationId": "ReporterAPI_GenericReport",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ReportResponse"
            },
            "examples": {
              "application/json": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1ReportRequest"
            }
          }
        ],
        "tags": [
          "ReporterAPI"
        ],
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ]
      }
    }
  },
  "definitions": {
    "GenericReportMetric": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "genericv1GenericReport": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Report UUID.\nRequired."
        },
        "createTime": {
          "type": "string",
          "format": "date-time",
          "description": "Time when this event was generated by the sender.\nRequired."
        },
        "instanceId": {
          "type": "string",
          "description": "Product instance unique identifier. UUID.\nRequired."
        },
        "productFamily": {
          "$ref": "#/definitions/v1ProductFamily",
          "description": "Product family.\nMandatory."
        },
        "metrics": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/GenericReportMetric"
          },
          "description": "Metrics collection.\nOptional."
        }
      },
      "description": "GenericReport contains set of metrics and service information.\nThe exact metrics pairs (metric name, metric value) is defined by each product."
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1ProductFamily": {
      "type": "string",
      "enum": [
        "PRODUCT_FAMILY_INVALID",
        "PRODUCT_FAMILY_PS",
        "PRODUCT_FAMILY_PXC",
        "PRODUCT_FAMILY_PSMDB",
        "PRODUCT_FAMILY_PBM",
        "PRODUCT_FAMILY_POSTGRESQL",
        "PRODUCT_FAMILY_PMM",
        "PRODUCT_FAMILY_EVEREST",
        "PRODUCT_FAMILY_PERCONA_TOOLKIT",
        "PRODUCT_FAMILY_PXB",
        "PRODUCT_FAMILY_OPERATOR_PXC",
        "PRODUCT_FAMILY_OPERATOR_POSTGRESQL",
        "PRODUCT_FAMILY_OPERATOR_PSMDB",
        "PRODUCT_FAMILY_OPERATOR_PS"
      ],
      "default": "PRODUCT_FAMILY_INVALID",
      "description": "ProductFamily represents Percona product family."
    },
    "v1ReportRequest": {
      "type": "object",
      "properties": {
        "reports": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/genericv1GenericReport"
          },
          "description": "One or more metric reports."
        }
      }
    },
    "v1ReportResponse": {
      "type": "object"
    }
  },
  "x-readme": {
    "samples-languages": [
